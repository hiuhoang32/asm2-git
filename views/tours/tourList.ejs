                                                            <a href="javascript:void(0)" class="single">User <img src="<%= user.profilePicture %>" alt="Profile Picture" id="sign-in-profile-picture" class="profile-picture"> <i
                                                                    class="ri-arrow-down-s-line"></i></a>
                                                            <ul class="submenu">
                                                                <li class="single-list">
                                                                    <a class="single">Username: <%= user.username %></a>
                                                                </li>
                                                                <li class="single-list">
                                                                   <a class="single">Balance: $<%= balance || '0' %></a>
                                                                </li>
