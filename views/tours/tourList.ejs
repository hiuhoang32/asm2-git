                                                                </form>
                                                                <li class="single-list">
                                                                    <a class="single" href="/user/logout">Log Out</a>
                                                                </li>
                                                            </ul>
                                                        </li>
                                                    <% } else { %>
                                                        <li class="single-list">
                                                            <a href="/user/login" class="single">Sign In</a>
                                                        </li>
                                                    <% } %>
                                                    <li class="d-block d-lg-none">
                                                        <div class="header-right pl-15">
                                                            <div class="d-flex align-items-center gap-12">
                                                                <div class="lang">
                                                                    <i class="ri-global-line"></i>
                                                                </div>
                                                                <div class="divider gradient-divider"></div>
                                                                <div class="money">
                                                                    <p class="pera">USD</p>
                                                                </div>
                                                            </div>
                                                            <div class="sign-btn">
                                                                <a href="<%= user ? '/user/logout' : '/user/login' %>" class="btn-secondary-sm"><%= user ? 'Sign Out' : 'Sign In' %></a>
                                                            </div>
                                                        </div>
                                                    </li>
                                                </ul>
                                                <div class="header-right">
                                                    <div class="d-flex align-items-center gap-12">
                                                        <div class="lang">
                                                            <i class="ri-global-line"></i>
                                                        </div>
                                                        <div class="divider gradient-divider"></div>
                                                        <div class="money">
                                                            <p class="pera">USD</p>
                                                        </div>
                                                    </div>
                                                    <div class="sign-btn">
                                                        <a href="<%= user ? '/user/logout' : '/user/login' %>" class="btn-secondary-sm"><%= user ? 'Sign Out' : 'Sign In' %></a>
                                                    </div>
                                                    <!-- Theme Mode -->
                                                    <li class="single-list">
                                                        <button class="ToggleThemeButton change-theme-mode m-0 p-0 border-0">
                                                            <i class="ri-sun-line"></i>
                                                        </button>
